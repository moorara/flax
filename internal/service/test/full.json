{
  "config": {
    "httpPort": 8080,
    "httpsPort": 8443
  },

  "http": [
    {
      "methods": [ "GET" ],
      "path": "/health",
      "statusCode": 200
    },
    {
      "methods": [ "GET" ],
      "path": "/current/user",
      "filters": {
        "params": {
          "tenantId": "\\w+"
        },
        "headers": {
          "Authorization": "Bearer .*"
        }
      },
      "delay": "100ms",
      "statusCode": 200,
      "headers": {
        "Tenant-ID": "4c85a46f-e11b-4d5f-bca4-add78c1f6395"
      },
      "body": { "id": "5da8349a-0707-4064-8fad-74cedb48a8fc", "name": "John Doe", "email": "john.doe@example.com" }
    }
  ],

  "rest": [
    {
      "name": "car",
      "basePath": "/api/v1",
      "store": [
        { "id": "ad2bd67b-172e-4778-a8a3-7cfb626685b9", "make": "Mazda", "model": "CX-5" },
        { "id": "26ee9c87-fdbb-48cf-be0a-add9a3d87189", "make": "Hyundai", "model": "Sonata" }
      ]
    },
    {
      "name": "team",
      "basePath": "/api/v1",
      "delay": "100ms",
      "identifier": "_id",
      "listHandle": "data",
      "filters": {
        "headers": {
          "Authorization": "Bearer .*"
        }
      },
      "headers": {
        "Tenant-ID": "4c85a46f-e11b-4d5f-bca4-add78c1f6395"
      },
      "store": [
        { "_id": "d93ce179-50f7-469e-bb36-1b3746145f00", "name": "Back-end", "tags": [ "go", "cloud" ] },
        { "_id": "8cd6ef6c-2095-4c75-bc66-6f38e785299d", "name": "Front-end", "tags": [ "react", "redux" ] }
      ]
    }
  ]
}
